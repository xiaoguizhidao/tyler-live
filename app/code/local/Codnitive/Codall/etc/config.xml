<config>
    <modules>
        <Codnitive_Codall>
            <version>1.1.16</version>
            <title>CODNITIVE Information</title>
            <link><![CDATA[http://www.codnitive.com/]]></link>
        </Codnitive_Codall>
    </modules>
	
    <global>
        <blocks>
            <codall>
                <class>Codnitive_Codall_Block</class>
            </codall>
        </blocks>
        <helpers>
            <codall>
                <class>Codnitive_Codall_Helper</class>
            </codall>
        </helpers>
        <models>
            <codall>
                <class>Codnitive_Codall_Model</class>
            </codall>
        </models>
        <resources>
            <codall_setup>
                <setup>
                    <module>Codnitive_Codall</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </codall_setup>
            <codall_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </codall_write>
            <codall_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </codall_read>
        </resources>
    </global>

    <adminhtml>
        <translate>
            <modules>
                <Codnitive_Codall>
                    <files>
                        <default>Codnitive_Codall.csv</default>
                    </files>
                </Codnitive_Codall>
            </modules>
        </translate>
    </adminhtml>
</config>